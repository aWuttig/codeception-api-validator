{
    "name": "awuttig/codeception-api-validator",
    "description": "Validate API Requests and Responses against Swagger / OpenAPI definitions",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@mailbox.org"
        }
    ],
    "require": {
        "codeception/codeception": "^5.0.0",
        "codeception/module-phpbrowser": "^3.0.0",
        "codeception/module-rest": "^3.0.0",
        "eleven-labs/api-validator": "~0.7.0",
        "symfony/serializer": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeception\\Module\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
