{
    "name": "awuttig/codeception-api-validator",
    "description": "Validate API Requests and Responses against Swagger / OpenAPI definitions",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@mailbox.org"
        }
    ],
    "require": {
        "codeception/codeception": "~2.3",
        "eleven-labs/api-validator": "~0.5.4",
        "symfony/serializer": "^3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "Awuttig\\Codeception\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awuttig\\Codeception\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
