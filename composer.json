{
    "name": "awuttig/codeception-api-validator",
    "description": "Validate API Requests and Responses against Swagger / OpenAPI definitions",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@mailbox.org"
        }
    ],
    "require": {
        "codeception/codeception": "^4.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-rest": "^1.0.0",
        "eleven-labs/api-validator": "~0.5.4",
        "symfony/serializer": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8"
    },
    "autoload":{
        "classmap": ["src/"]
    },
    "config": {
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
